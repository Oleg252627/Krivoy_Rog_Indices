@model IEnumerable<Client.Models.Street>

@{
    ViewBag.Title = "Deteils_Serch";
}

<h2>Таблица улиц по индексу: @ViewBag.Name_index</h2>

<p>
    @Html.ActionLink("Таблица индексов", "Index", null, new { @class = "index_table_linc" })
</p>
<div class="table_block">
    <table class="table table-condensed table-striped">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
        
            </tr>
        }

    </table>
</div>

